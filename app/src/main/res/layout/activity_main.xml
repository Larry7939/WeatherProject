<?xml version="1.0" encoding="utf-8"?>

<androidx.drawerlayout.widget.DrawerLayout
    android:id="@+id/drawerLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="20dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >
<com.baoyz.widget.PullRefreshLayout
    android:id="@+id/swipeRefreshLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:refreshType="water_drop"
    android:background="@drawable/clear_background1"
    >

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    //배경1(동적-날씨,시간)
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/back1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/clear_background1"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    //배경2(동적)
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/back2"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/clear_background2"
        android:ellipsize = "marquee"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    //메뉴버튼
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        app:srcCompat="@drawable/top_menu"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    //검색버튼
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/top_search"
        android:layout_width="30dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        app:srcCompat="@drawable/search"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    //위치(동적)
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="30dp"
        tools:text="서울특별시 동대문구"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:shadowColor="@color/black"
        android:shadowRadius="10"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    //날씨 아이콘
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/weather_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="120dp"
        android:layout_marginStart="70dp"
        tools:srcCompat="@drawable/clear">

    </androidx.appcompat.widget.AppCompatImageView>
    //날씨 상태
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/weather_state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/weather_icon"
        app:layout_constraintStart_toStartOf="@id/weather_icon"
        app:layout_constraintEnd_toEndOf="@id/weather_icon"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="2dp"
        tools:text="맑음"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:shadowColor="@color/black"
        android:shadowRadius="10">
    </androidx.appcompat.widget.AppCompatTextView>
    //현재온도
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/current_temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="90dp"
        tools:text="34"
        android:textSize="60sp"
        android:textColor="@color/white"
        android:textStyle=""
        android:shadowColor="@color/black"
        android:shadowRadius="10"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    //최고기온(기호)
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/current_temp"
        android:layout_marginTop="130dp"
        android:layout_marginStart="10dp"
        android:text="↑"
        android:textColor="#B7A968"
        android:textStyle="bold"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    //최저기온(기호)
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/current_temp"
        android:layout_marginTop="155dp"
        android:layout_marginStart="10dp"
        android:text="↓"
        android:textColor="#61A6D6"
        android:textStyle="bold"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    //최고기온(온도,동적)
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/max_temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/current_temp"
        android:layout_marginTop="128dp"
        android:layout_marginStart="30dp"
        tools:text="35°"
        android:textSize="15sp"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:shadowColor="@color/black"
        android:shadowRadius="10"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    //최저기온(온도,동적)
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/min_temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/current_temp"
        android:layout_marginTop="153dp"
        android:layout_marginStart="30dp"
        tools:text="24°"
        android:textSize="15sp"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:shadowColor="@color/black"
        android:shadowRadius="10"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    //어제 온도와 비교
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/compare_yesterday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="어제와 같아요"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="195dp"
        android:layout_marginStart="240dp"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:textStyle=""
        android:shadowColor="@color/black"
        android:shadowRadius="10">
    </androidx.appcompat.widget.AppCompatTextView>
    //체감온도(동적)
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/feel_like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="체감온도 38°"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="230dp"
        android:layout_marginStart="245dp"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:shadowColor="@color/black"
        android:shadowRadius="10">
    </androidx.appcompat.widget.AppCompatTextView>
    //미세먼지,바람,습도,강수확률 아이콘
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/wind_blade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon_linear"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="50dp"
        app:srcCompat="@drawable/wind_blade">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/icon_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/back2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        android:weightSum="4"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/must"
            android:layout_width="111dp"
            android:layout_height="match_parent"
            tools:srcCompat="@drawable/must_normal"
            >
        </androidx.appcompat.widget.AppCompatImageView>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/wind"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/wind_body"
            >
        </androidx.appcompat.widget.AppCompatImageView>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/humadity"
            android:layout_width="95dp"
            android:layout_height="match_parent"
            tools:srcCompat="@drawable/humadity_46"
            >
        </androidx.appcompat.widget.AppCompatImageView>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/rain"
            android:layout_width="105dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/rain_80"
            >
        </androidx.appcompat.widget.AppCompatImageView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    //미세먼지,바람,습도,강수확률 수치
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/degree_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/icon_linear"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        >
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/must_value"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            tools:text = "보통"
            android:textSize="17sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center"
            >
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/wind_value"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            tools:text = "고요함"
            android:textSize="17sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/humadity_value"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            tools:text = "49%"
            android:textSize="17sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/rain_value"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            tools:text = "0.0m"
            android:textSize="17sp"
            android:textColor="@color/white"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center">
        </androidx.appcompat.widget.AppCompatTextView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/display_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/degree_linear"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        >
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/must_display"
            android:layout_width="111dp"
            android:layout_height="wrap_content"
            android:text = "미세먼지"
            android:textSize="12sp"
            android:textColor="#CED1DA"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center"
            >
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/wind_display"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text = "바람"
            android:textSize="12sp"
            android:textColor="#CED1DA"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center"
            >
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/humadity_display"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text = "습도"
            android:textSize="12sp"
            android:textColor="#CED1DA"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center"
            >
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/rain_display"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            android:text = "강수량"
            android:textSize="12sp"
            android:textColor="#CED1DA"
            android:shadowColor="@color/black"
            android:shadowRadius="10"
            android:gravity="center"
            >
        </androidx.appcompat.widget.AppCompatTextView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvForvp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@id/imageview_viewpager"
        tools:text = ""
        android:shadowColor="#000000"
        android:shadowRadius="10"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:layout_marginBottom="10dp">
    </androidx.appcompat.widget.AppCompatTextView>


    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageview_viewpager"
        android:layout_width="390dp"
        android:layout_height="320dp"
        android:background="@drawable/custom_imageview_viewpager"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="10dp"
        >
    </androidx.appcompat.widget.AppCompatImageView>

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/viewPagerBook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="10dp"
        android:paddingStart="13dp"
        android:paddingEnd="8dp"
        android:orientation="horizontal">
    </androidx.viewpager2.widget.ViewPager2>

</androidx.constraintlayout.widget.ConstraintLayout>
</com.baoyz.widget.PullRefreshLayout>

    //drawer
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/drawerConst"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="#252325"
    android:layout_gravity="start">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/drawer_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/drawer_background"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/drawer_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/drawer_background"
        android:layout_marginStart="30dp"
        app:srcCompat="@drawable/icon_drawer"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="첫화면날씨"
        android:textSize="23sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="@id/drawer_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/drawer_background"
        android:layout_marginStart="80dp"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="기온 단위"
        app:layout_constraintTop_toBottomOf="@id/drawer_background"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/unit_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/drawer_background"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="17dp"
        android:layout_marginStart="100dp"
        app:srcCompat="@drawable/unit_c_on"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/drawer_background"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        app:srcCompat="@drawable/drawer_menu">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="380dp"
        app:layout_constraintTop_toBottomOf="@+id/unit_button"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="70dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        android:weightSum="6">
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/interested"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="관심 지역"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_weight="1"
        android:gravity="center">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="미세 및 기상지도"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_weight="1"
        android:gravity="center">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="뉴스"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_weight="1"
        android:gravity="center">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="친구에게 앱 알려주기"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_weight="1"
        android:gravity="center">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="풀버전 구매(광고 제거)"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_weight="1"
        android:gravity="center">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="설정"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/white"
        android:layout_weight="1"
        android:gravity="center">
    </androidx.appcompat.widget.AppCompatTextView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="#403F3F"
        android:layout_marginTop="340dp"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintStart_toStartOf="parent"
        android:text="나에게 딱 맞는 앱"
        android:textSize="12sp"
        android:textColor="#8C8C8C"
        android:textStyle="bold"
        android:layout_marginTop="15dp"
        android:layout_marginStart="28dp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="50dp"
        android:layout_marginStart="23dp"
        app:srcCompat="@drawable/drawer_menu2">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="54dp"
        android:layout_marginStart="75dp"
        android:text="말하는 번역기"
